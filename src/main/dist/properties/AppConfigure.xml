<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
    <bean id="dao" class="edu.mcw.rgd.DAO">
        <property name="excludedOntologyAspects" >
            <set>
                <value>E</value> <!-- CHEBI -->
                <value>H</value> <!-- HPO -->
                <value>N</value> <!-- MP -->
            </set>
        </property>
    </bean>

    <bean id="loader" class="edu.mcw.rgd.Loader">
        <property name="version" value="Transitive Annotation pipeline  -- build Aug 31, 2020" />
        <property name="createdBy" value="67" />
        <property name="refRgdId" value="9068941"/><!--2298974 - test reference -->
        <property name="dao" ref="dao"/>
        <property name="evidenceCode" value="ISO" /><!-- evidence code for generated transitive annotations -->
        <property name="inputEvidenceCodes"><!-- evidence codes for source manual annotations -->
            <list>
                <value>EXP</value>
                <value>IDA</value>
                <value>IPI</value>
                <value>IMP</value>
                <value>IGI</value>
                <value>IEP</value>
                <value>IAGP</value>
                <value>HTP</value>
                <value>HAD</value>
                <value>HMP</value>
                <value>HGI</value>
                <value>HEP</value>
            </list>
        </property>
        <property name="processedSpecies" >
            <list>
                <value>rat</value>
                <value>mouse</value>
                <value>human</value>
                <value>bonobo</value>
                <value>chinchilla</value>
                <value>dog</value>
                <value>squirrel</value>
                <value>pig</value>
                <value>vervet</value>
                <value>molerat</value>
            </list>
        </property>
    </bean>
</beans>