<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
    <bean id="dao" class="edu.mcw.rgd.DAO">
        <property name="excludedOntologyAspects" >
            <set>
                <value>E</value> <!-- CHEBI -->
                <value>H</value> <!-- HPO -->
                <value>N</value> <!-- MP -->
            </set>
        </property>
    </bean>

    <bean id="loader" class="edu.mcw.rgd.Loader">
        <property name="version" value="Transitive Annotation pipeline  -- build Jun 15, 2021" />
        <property name="createdBy" value="67" />
        <property name="refRgdId" value="9068941"/><!--2298974 - test reference -->
        <property name="dao" ref="dao"/>
        <property name="evidenceCode" value="ISO" /><!-- evidence code for generated transitive annotations -->
        <property name="inputEvidenceCodes"><!-- evidence codes for source manual annotations -->
            <list>
                <value>EXP</value>
                <value>IDA</value>
                <value>IPI</value>
                <value>IMP</value>
                <value>IGI</value>
                <value>IEP</value>
                <value>IAGP</value>
                <value>HTP</value>
                <value>HAD</value>
                <value>HMP</value>
                <value>HGI</value>
                <value>HEP</value>
            </list>
        </property>
        <property name="processedSpeciesTypeKeys" >
            <set>
                <value>3</value><!-- rat -->
                <value>2</value><!-- mouse -->
                <value>1</value><!-- human -->
                <value>5</value><!-- bonobo -->
                <value>4</value><!-- chinchilla -->
                <value>6</value><!-- dog -->
                <value>7</value><!-- squirrel -->
                <value>9</value><!-- pig -->
                <value>13</value><!-- vervet - African green monkey -->
                <value>14</value><!-- naked mole rat -->
            </set>
        </property>
    </bean>
</beans>