<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
    <bean id="manager" class="edu.mcw.rgd.Manager">
        <property name="version" value="Transitive Annotation pipeline  -- build May 11, 2020" />
        <property name="pipelineName" value="Transitive Annotation Pipeline" />
        <property name="createdBy" value="67" />
        <property name="refRgdId" value="9068941" />
        <property name="evidenceCode" value="ISS" />
        <property name="speciesProcessed" >
            <list>
                <value>bonobo</value>
                <value>chinchilla</value>
                <value>dog</value>
                <value>squirrel</value>
                <value>pig</value>
                <value>vervet</value>
                <value>molerat</value>
            </list>
        </property>
        <property name="dao" ref="dao"/>
    </bean>

    <bean id="dao" class="edu.mcw.rgd.DAO">
        <property name="excludedOntologyAspects" >
            <set>
                <value>E</value> <!-- CHEBI -->
                <value>H</value> <!-- HPO -->
                <value>N</value> <!-- MP -->
            </set>
        </property>
    </bean>
</beans>